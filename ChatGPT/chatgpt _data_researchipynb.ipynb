    "import pandas as pd\n",
    "import numpy as np\n",
    "import requests\n",
    "from datetime import datetime\n",
    "from sklearn.preprocessing import MinMaxScaler\n",
    "import yfinance as yf\n",
    "\n",
    "#=============================================================\n",
    "# A. 헬퍼 함수: 멀티 인덱스 → 단일 컬럼으로 평탄화\n",
    "#=============================================================\n",
    "def flatten_columns(df: pd.DataFrame) -> pd.DataFrame:\n",
    "    \"\"\"\n",
    "    만약 df.columns가 MultiIndex라면, (\"Open\",\"\") 형태를 \"Open\" 형태로 변환.\n",
    "    df.reset_index()도 수행하여 인덱스를 일반 컬럼으로 만든다.\n",
    "    \"\"\"\n",
    "    if isinstance(df.columns, pd.MultiIndex):\n",
    "        # 예: (('Open',''),('Close','')) 처럼 튜플이면 join해서 문자열로 만든다.\n",
    "        df.columns = ['_'.join([str(c) for c in col_tuple if c]) for col_tuple in df.columns.values]\n",
    "    else:\n",
    "        df.columns = [str(c) for c in df.columns]  # 혹시 모를 타입 불일치 방지\n",
    "\n",
    "    # index -> column\n",
    "    df.reset_index(drop=False, inplace=True)\n",
    "    return df\n",
    "\n",
    "#=============================================================\n",
    "# B. 데이터 수집\n",
    "#=============================================================\n",
    "start_date = \"2018-01-01\"\n",
    "end_date   = \"2023-12-31\"\n",
    "\n",
    "#---------------------------\n",
    "# (1) 비트코인 시세 (BTC-USD)\n",
    "#---------------------------\n",
    "btc_df = yf.download(\"BTC-USD\", start=start_date, end=end_date, progress=False)\n",
    "btc_df.rename(columns={\n",
    "    \"Open\": \"btc_open\",\n",
    "    \"High\": \"btc_high\",\n",
    "    \"Low\": \"btc_low\",\n",
    "    \"Close\": \"btc_close\",\n",
    "    \"Volume\": \"btc_volume\",\n",
    "    \"Adj Close\": \"btc_adj_close\"\n",
    "}, inplace=True)\n",
    "btc_df = flatten_columns(btc_df)  # MultiIndex -> 단일 컬럼, 인덱스 리셋\n",
    "\n",
    "#---------------------------\n",
    "# (2) S&P500 (^GSPC)\n",
    "#---------------------------\n",
    "sp500_df = yf.download(\"^GSPC\", start=start_date, end=end_date, progress=False)\n",
    "sp500_df.rename(columns={\n",
    "    \"Open\": \"sp500_open\",\n",
    "    \"High\": \"sp500_high\",\n",
    "    \"Low\": \"sp500_low\",\n",
    "    \"Close\": \"sp500_close\",\n",
    "    \"Volume\": \"sp500_volume\"\n",
    "}, inplace=True)\n",
    "sp500_df = flatten_columns(sp500_df)\n",
    "\n",
    "#---------------------------\n",
    "# (3) 달러 인덱스 (DXY: DX-Y.NYB) - 데이터 없으면 \"DX=F\" 등 시도\n",
    "#---------------------------\n",
    "dxy_df = yf.download(\"DX-Y.NYB\", start=start_date, end=end_date, progress=False)\n",
    "dxy_df.rename(columns={\n",
    "    \"Open\": \"dxy_open\",\n",
    "    \"High\": \"dxy_high\",\n",
    "    \"Low\": \"dxy_low\",\n",
    "    \"Close\": \"dxy_close\",\n",
    "    \"Volume\": \"dxy_volume\"\n",
    "}, inplace=True)\n",
    "dxy_df = flatten_columns(dxy_df)\n",
    "\n",
    "#---------------------------\n",
    "# (4) 10년물 국채금리 (^TNX)\n",
    "#---------------------------\n",
    "tnx_df = yf.download(\"^TNX\", start=start_date, end=end_date, progress=False)\n",
    "tnx_df.rename(columns={\n",
    "    \"Open\": \"tnx_open\",\n",
    "    \"High\": \"tnx_high\",\n",
    "    \"Low\": \"tnx_low\",\n",
    "    \"Close\": \"tnx_close\"\n",
    "}, inplace=True)\n",
    "tnx_df = flatten_columns(tnx_df)\n",
    "\n",
    "#---------------------------\n",
    "# (5) 공포·탐욕 지수 (Fear & Greed Index)\n",
    "#---------------------------\n",
    "fng_url = \"https://api.alternative.me/fng/?limit=2000&format=json&date_format=us\"\n",
    "resp = requests.get(fng_url)\n",
    "if resp.status_code == 200:\n",
    "    fng_data = resp.json().get('data', [])\n",
    "    fear_greed_df = pd.DataFrame(fng_data)\n",
    "    fear_greed_df['value'] = pd.to_numeric(fear_greed_df['value'], errors='coerce')\n",
    "    fear_greed_df.rename(columns={'value': 'fear_greed', 'timestamp': 'Date'}, inplace=True)\n",
    "    fear_greed_df['Date'] = pd.to_datetime(fear_greed_df['Date'], format='%m-%d-%Y', errors='coerce')\n",
    "    fear_greed_df.dropna(subset=['Date'], inplace=True)\n",
    "    # 혹시 MultiIndex가 생겼을 가능성은 낮지만, 동일하게 처리\n",
    "    fear_greed_df = flatten_columns(fear_greed_df)\n",
    "else:\n",
    "    print(\"Fear & Greed API 실패, 빈 DF 생성\")\n",
    "    fear_greed_df = pd.DataFrame(columns=['Date','fear_greed'])\n",
    "\n",
    "#=============================================================\n",
    "# C. 병합(merge): 모든 DF가 'Date'를 일반 컬럼으로 가지고 있을 때만\n",
    "#=============================================================\n",
    "df = pd.merge(btc_df, sp500_df, on='Date', how='outer')\n",
    "df = pd.merge(df, dxy_df, on='Date', how='outer')\n",
    "df = pd.merge(df, tnx_df, on='Date', how='outer')\n",
    "df = pd.merge(df, fear_greed_df, on='Date', how='outer')\n",
    "\n",
    "#=============================================================\n",
    "# D. 전처리(결측치 & 이상치)\n",
    "#=============================================================\n",
    "df['Date'] = pd.to_datetime(df['Date'])\n",
    "df.sort_values(by='Date', inplace=True)\n",
    "\n",
    "# 결측치\n",
    "df.fillna(method='ffill', inplace=True)\n",
    "df.fillna(method='bfill', inplace=True)\n",
    "\n",
    "# 이상치 예시\n",
    "if 'btc_volume' in df.columns:\n",
    "    df = df[df['btc_volume'] >= 0]\n",
    "\n",
    "#=============================================================\n",
    "# E. 최종적으로만 set_index('Date')\n",
    "#=============================================================\n",
    "df.set_index('Date', inplace=True)\n",
    "\n",
    "print(\"최종 DF shape:\", df.shape)\n",
    "print(df.head(5))\n",
    "\n",
    "#=============================================================\n",
    "# F. 스케일링(MinMaxScaler)\n",
    "#=============================================================\n",
    "from sklearn.preprocessing import MinMaxScaler\n",
    "scale_cols = [\"btc_close\",\"btc_volume\",\"sp500_close\",\"dxy_close\",\"tnx_close\",\"fear_greed\"]\n",
    "scaler = MinMaxScaler()\n",
    "for c in scale_cols:\n",
    "    if c in df.columns:\n",
    "        df[c] = scaler.fit_transform(df[[c]])\n",
    "\n",
    "#=============================================================\n",
    "# G. Train/Val/Test 분할\n",
    "#=============================================================\n",
    "train = df.loc[\"2018-01-01\":\"2022-12-31\"].copy()\n",
    "val   = df.loc[\"2023-01-01\":\"2023-06-30\"].copy()\n",
    "test  = df.loc[\"2023-07-01\":\"2023-12-31\"].copy()\n",
    "\n",
    "print(\"train shape:\", train.shape)\n",
    "print(\"val shape:\",   val.shape)\n",
    "print(\"test shape:\",  test.shape)\n",
    "\n",
    "#=============================================================\n",
    "# H. CSV 저장(Colab /content/)\n",
    "#=============================================================\n",
    "df.to_csv('/content/final_data.csv', index=True, encoding='utf-8-sig')\n",
    "print(\"CSV 저장 완료:\", '/content/final_data.csv')\n"
   ]
  },
